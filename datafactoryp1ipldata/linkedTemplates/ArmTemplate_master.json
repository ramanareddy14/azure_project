{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactoryp1ipldata"
		},
		"LS_AzureDataLakeStorageG2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_AzureDataLakeStorageG2'"
		},
		"LS_AzureSqlDatabase_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_AzureSqlDatabase'"
		},
		"LS_AzureDataLakeStorageG2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsproject1ipldata.dfs.core.windows.net/"
		},
		"LS_http_for_git_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://github.com/ramanareddy14/project1_ipldata_adf/raw/main/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafactoryp1ipldata_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_AzureDataLakeStorageG2_accountKey": {
						"value": "[parameters('LS_AzureDataLakeStorageG2_accountKey')]"
					},
					"LS_AzureSqlDatabase_connectionString": {
						"value": "[parameters('LS_AzureSqlDatabase_connectionString')]"
					},
					"LS_AzureDataLakeStorageG2_properties_typeProperties_url": {
						"value": "[parameters('LS_AzureDataLakeStorageG2_properties_typeProperties_url')]"
					},
					"LS_http_for_git_properties_typeProperties_url": {
						"value": "[parameters('LS_http_for_git_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}